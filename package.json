{
  "name": "tuspacio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon -L",
    "build": "npm run quality:check && npm run prepare",
    "build:prod": "NODE_ENV=production npm run build",
    "build:vercel": "echo 'Skipping quality checks for Vercel build' && npm run prepare",
    "build:skip-checks": "npm run prepare || echo 'Build completed with warnings'",
    "build:force": "echo 'Forcing build without any checks' || true",
    "vercel-build": "npm run build:vercel",
    "db:sync": "node sync-database.js",
    "db:seed": "node src/scripts/seed-initial-data.js",
    "db:seed:initial": "node src/scripts/seed-initial-data.js",
    "db:seed:products": "node src/scripts/seed-products.js",
    "db:seed:all": "npm run db:seed:initial && npm run db:seed:products",
    "db:seed:complete": "node src/scripts/seed-complete.js",
    "test": "mocha --recursive ./tests/**/*.spec.js --exit",
    "test:watch": "mocha --recursive ./tests/**/*.spec.js --watch",
    "test:coverage": "nyc mocha --recursive ./tests/**/*.spec.js --exit",
    "test:property": "mocha --recursive ./tests/property-tests/**/*.spec.js --exit",
    "test:api": "node test-api.js",
    "test:ci": "npm run lint && npm run test",
    "compare:apis": "node compare-apis.js",
    "json": "json-server --watch src/controllers/db.json --port 3004",
    "dev:full": "concurrently \"npm run json\" \"npm run dev\"",
    "lint": "eslint src/**/*.js tests/**/*.js",
    "lint:fix": "eslint src/**/*.js tests/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\" \"tests/**/*.js\" \"*.js\" \"*.json\" \"*.md\"",
    "format:check": "prettier --check \"src/**/*.js\" \"tests/**/*.js\" \"*.js\" \"*.json\" \"*.md\"",
    "quality": "npm run lint && npm run format:check && npm run test",
    "quality:check": "npm run lint && npm run format:check",
    "prepare": "node scripts/prepare.js || echo 'Prepare script completed with warnings'",
    "postinstall": "echo 'Dependencies installed successfully'",
    "install:clean": "npm install --no-audit --no-fund --quiet",
    "install:silent": "npm install --no-audit --no-fund --silent",
    "ci:install": "npm ci --no-audit --no-fund --quiet",
    "pre-deploy": "node scripts/pre-deploy-check.js",
    "deploy:vercel": "npm run pre-deploy && vercel --prod"
  },
  "author": "Enderson Millan",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "concurrently": "^9.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-joi-validation": "^5.0.1",
    "express-openid-connect": "^2.19.3",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.18.2",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "json-server": "^1.0.0-beta.3",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.11",
    "nyc": "^17.1.0",
    "pg": "^8.13.1",
    "sequelize": "^6.37.5",
    "stripe": "^17.3.1"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "fast-check": "^3.23.1",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "semver": "^7.7.3",
    "sinon": "^21.0.0",
    "standard": "^17.1.2",
    "supertest": "^7.0.0",
    "supertest-session": "^5.0.1"
  },
  "eslintConfig": {
    "extends": "./node_modules/standard/eslintrc.json"
  }
}
