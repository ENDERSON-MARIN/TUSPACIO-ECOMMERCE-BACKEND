{
  "all": true,
  "include": ["src/**/*.js"],
  "exclude": ["tests/**", "node_modules/**", "coverage/**", "*.config.js"],
  "reporter": ["text", "html", "lcov"],
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80,
  "cache": false,
  "temp-dir": "./coverage/.nyc_output"
}
